module
{

    namespace Elastos {
    namespace Droid {
    namespace Bluetooth {

        /**
         * @hide
         */
        /**
         * @Involve
         * interface IParcelable
         */
        interface IBluetoothMasInstance {
            GetId(
                [out] Int32* id);

            GetName(
                [out] String* name);

            GetChannel(
                [out] Int32* channel);

            GetMsgTypes(
                [out] Int32* types);

            MsgSupported(
                [in] Int32 msg,
                [out] Boolean* result);
        }

        interface IBluetoothMasInstanceMessageType {
            const Int32 EMAIL    = 0x01;
            const Int32 SMS_GSM  = 0x02;
            const Int32 SMS_CDMA = 0x04;
            const Int32 MMS      = 0x08;
        }

    } // Bluetooth
    } // Droid
    } // Elastos
}
