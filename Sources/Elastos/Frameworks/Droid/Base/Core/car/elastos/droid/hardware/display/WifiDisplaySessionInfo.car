
module
{
    namespace Elastos {
    namespace Droid {
    namespace Hardware {
    namespace Display {

    /**
     * This class contains information regarding a wifi display session
     * (such as session id, source ip address, etc.). This is needed for
     * Wifi Display Certification process.
     * <p>
     * This object is immutable.
     * </p>
     *
     * @hide
     *
     * @Involve
     * interface IParcelable;
     */
    interface IWifiDisplaySessionInfo {
        IsClient(
            [out] Boolean* result);

        GetSessionId(
            [out] Int32* id);

        GetGroupId(
            [out] String* gId);

        GetPassphrase(
            [out] String* result);

        GetIP(
            [out] String* ip);

        ToString(
            [out] String* result);
    }

    } // namespace Display
    } // namespace Hardware
    } // namespace Droid
    } // namespace Elastos
}
