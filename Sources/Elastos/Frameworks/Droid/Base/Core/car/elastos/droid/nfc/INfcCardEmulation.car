    interface IINfcCardEmulation {
        IsDefaultServiceForCategory(
            [in] Int32 userHandle,
            [in] IComponentName* service,
            [in] String category,
            [out] Boolean* res);

        IsDefaultServiceForAid(
            [in] Int32 userHandle,
            [in] IComponentName* service,
            [in] String aid,
            [out] Boolean* res);

        SetDefaultServiceForCategory(
            [in] Int32 userHandle,
            [in] IComponentName* service,
            [in] String category,
            [out] Boolean* res);

        SetDefaultForNextTap(
            [in] Int32 userHandle,
            [in] IComponentName* service,
            [out] Boolean* res);

        RegisterAidGroupForService(
            [in] Int32 userHandle,
            [in] IComponentName* service,
            [in] IAidGroup* aidGroup,
            [out] Boolean* res);

        GetAidGroupForService(
            [in] Int32 userHandle,
            [in] IComponentName* service,
            [in] String category,
            [out] IAidGroup** res);

        RemoveAidGroupForService(
            [in] Int32 userHandle,
            [in] IComponentName* service,
            [in] String category,
            [out] Boolean* res);

        GetServices(
            [in] Int32 userHandle,
            [in] String category,
            [out] IList** res );

        SetPreferredService(
            [in] IComponentName* service,
            [out] Boolean* res);

        UnsetPreferredService(
            [out] Boolean* res);

        SupportsAidPrefixRegistration(
            [out] Boolean* res);
