
module
{
    interface Elastos.Droid.Os.IBinder;
    interface Elastos.Droid.Content.IComponentName;
    interface Elastos.Droid.Content.Pm.IApplicationInfo;
    interface Elastos.Droid.Content.Res.ICompatibilityInfo;

    namespace Elastos {
    namespace Droid {
    namespace App {

    /** @hide */
    interface IReceiverDispatcher {
    }

    /** @hide */
    interface IServiceDispatcher {
        Connected(
           [in] IComponentName* name,
           [in] IBinder* service);

        Death(
           [in] IComponentName* name,
           [in] IBinder* service);

        DoConnected(
           [in] IComponentName* name,
           [in] IBinder* service);

        DoDeath(
           [in] IComponentName* name,
           [in] IBinder* service);
    }

    /**
     * Local state maintained about a currently loaded .apk.
     * @hide
     */
    interface ILoadedPkg {

        GetPackageName(
            [out] String* info);

        GetApplicationInfo(
            [out] IApplicationInfo** info);

        IsSecurityViolation(
            [out] Boolean* result);

        GetCompatibilityInfo(
            [out] ICompatibilityInfo** info);

        SetCompatibilityInfo(
            [in] ICompatibilityInfo* compatInfo);
    }

    } // namespace App
    } // namespace Droid
    } // namespace Elastos
}