
module
{
    interface Elastos.Droid.Internal.Os.IBatteryStatsImplUidProc;

    namespace Elastos {
    namespace Droid {
    namespace Internal {
    namespace Os {

    [local]
    interface IProcessCpuTrackerStats {
        GetPid(
            [out] Int32* pid);

        GetUid(
            [out] Int32* uid);

        GetBatteryStats(
            [out] IBatteryStatsImplUidProc** batteryStats);

        SetBatteryStats(
            [in] IBatteryStatsImplUidProc* batteryStats);

        GetInteresting(
            [out] Boolean* interesting);

        SetInteresting(
            [in] Boolean interesting);

        GetBaseName(
            [out] String* name);

        SetBaseName(
            [in] String baseName);

        GetName(
            [out] String* name);

        SetName(
            [in] String name);

        GetNameWidth(
            [out] Int32* width);

        SetNameWidth(
            [in] Int32 nameWidth);

        GetVsize(
            [out] Int64* vsize);

        SetVsize(
            [in] Int64 vsize);

        GetBaseUptime(
            [out] Int64* time);

        SetBaseUptime(
            [in] Int64 baseUptime);

        GetRelUptime(
            [out] Int64* time);

        SetRelUptime(
            [in] Int64 relUptime);

        GetBaseUtime(
            [out] Int64* time);

        SetBaseUtime(
            [in] Int64 baseUtime);

        GetBaseStime(
            [out] Int64* time);

        SetBaseStime(
            [in] Int64 baseStime);

        GetRelUtime(
            [out] Int32* time);

        SetRelUtime(
            [in] Int32 relUtime);

        GetRelStime(
            [out] Int32* time);

        SetRelStime(
            [in] Int32 relStime);

        GetBaseMinfaults(
            [out] Int64* faults);

        SetBaseMinfaults(
            [in] Int64 baseMinfaults);

        GetBaseMajfaults(
            [out] Int64* faults);

        SetBaseMajfaults(
            [in] Int64 baseMajfaults);

        GetRelMinfaults(
            [out] Int32* faults);

        SetRelMinfaults(
            [in] Int32 relMinfaults);

        GetRelMajfaults(
            [out] Int32* faults);

        SetRelMajfaults(
            [in] Int32 relMajfaults);

        GetActive(
            [out] Boolean* active);

        SetActive(
            [in] Boolean active);

        GetWorking(
            [out] Boolean* working);

        SetWorking(
            [in] Boolean working);

        GetAdded(
            [out] Boolean* added);

        SetAdded(
            [in] Boolean added);

        GetRemoved(
            [out] Boolean* removed);

        SetRemoved(
            [in] Boolean removed);
    }

    [local]
    interface IProcessCpuTracker {
        OnLoadChanged(
            [in] Float load1,
            [in] Float load5,
            [in] Float load15);

        OnMeasureProcessName(
            [in] String name,
            [out] Int32* result);

        Init();

        Update();

        /**
         * Returns the total time (in clock ticks, or 1/100 sec) spent executing in
         * both user and system code.  Safe to call without lock held.
         */
        GetCpuTimeForPid(
            [in] Int32 pid,
            [out] Int64* time);

        /**
         * Returns the delta time (in clock ticks, or 1/100 sec) spent at each CPU
         * speed, since the last call to this method. If this is the first call, it
         * will return 1 for each value.
         */
        GetLastCpuSpeedTimes(
            [out, callee] ArrayOf<Int64>* times);

        GetLastUserTime(
            [out] Int32* result);

        GetLastSystemTime(
            [out] Int32* result);

        GetLastIoWaitTime(
            [out] Int32* result);

        GetLastIrqTime(
            [out] Int32* result);

        GetLastSoftIrqTime(
            [out] Int32* result);

        GetLastIdleTime(
            [out] Int32* result);

        GetTotalCpuPercent(
            [out] Float* result);

        CountStats(
            [out] Int32* result);

        GetStats(
            [in] Int32 index,
            [out] IProcessCpuTrackerStats** stats);

        CountWorkingStats(
            [out] Int32* result);

        GetWorkingStats(
            [in] Int32 index,
            [out] IProcessCpuTrackerStats** stats);

        PrintCurrentLoad(
            [out] String* str);

        PrintCurrentState(
            [in] Int64 now,
            [out] String* str);
    }

    }//namespace Os
    }//namespace Internal
    }//namespace Droid
    }//namespace Elastos
}

