
module
{

    interface Elastos.Droid.NFC.ITag;

    namespace Elastos {
    namespace Droid {
    namespace NFC{
    namespace Tech{

    /**
     * @Involve
     * interace IBasicTagTechnology
     */
    [deprecated]
    interface IMifareClassic {
        GetType(
            [out] Int32* type);

        GetSize(
            [out] Int32* size);

        IsEmulated(
            [out] Boolean* emulated);

        GetSectorCount(
            [out] Int32* count);

        GetBlockCount(
            [out] Int32* count);

        GetBlockCountInSector(
            [in] Int32 sectorIndex,
            [out] Int32* type);

        BlockToSector(
            [in] Int32 blockIndex,
            [out] Int32* type);

