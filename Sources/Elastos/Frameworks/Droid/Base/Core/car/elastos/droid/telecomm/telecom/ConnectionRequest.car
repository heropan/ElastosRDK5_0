
module
{

    interface Elastos.Droid.Net.IUri;
    interface Elastos.Droid.Os.IBundle;

    namespace Elastos {
    namespace Droid {
    namespace Telecomm {
    namespace Telecom {

        /**
         * Simple data container encapsulating a request to some entity to
         * create a new {@link Connection}.
         * @hide
         */
        /**
         * @Involve
         * interface IParcelable
         */
        interface IConnectionRequest {
            GetAccountHandle(
                [out] IPhoneAccountHandle** res);

            GetAddress(
                [out] IUri** res);

            GetExtras(
                [out] IBundle** res);

            GetVideoState(
                [out] Int32* res);
        }

    } // namespace Telecom
    } // namespace Telecomm
    } // namespace Droid
    } // namespace Elastos
}
