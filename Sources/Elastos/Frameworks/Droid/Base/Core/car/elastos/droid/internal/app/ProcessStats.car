module
{
    using interface Elastos.IO.IInputStream;
    using interface Elastos.IO.IPrintWriter;
    using interface Elastos.Utility.IArrayList;
    interface Elastos.Droid.Utility.IArrayMap;

    namespace Elastos {
    namespace Droid {
