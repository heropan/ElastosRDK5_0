    namespace App {

    interface IProcessStats;

    [local, deprecated]
    interface IDurationsTable {
        GetStats(
            [out] IProcessStats** stats);

        GetName(
            [out] String* name);

        GetDurationsTable(
            [out, callee] ArrayOf<Int32>* durationsTable);

        SetDurationsTable(
            [in] ArrayOf<Int32>* durationsTable);

        GetDurationsTableSize(
            [out] Int32* durationsTableSize);

        SetDurationsTableSize(
            [in] Int32 durationsTableSize);
    }

    /**
     * @Involve
     * interface IDurationsTable
     */
    [local]
    interface IProcessState {
        MakeActive();

        MakeInactive();

        IsInUse(
            [out] Boolean* isInUse);

        /**
         * Update the current state of the given list of processes.
         *
         * @param state Current ActivityManager.PROCESS_STATE_*
         * @param memFactor Current mem factor constant.
         * @param now Current time.
         * @param pkgList Processes to update.
         */
        SetState(
            [in] Int32 state,
            [in] Int32 memFactor,
            [in] Int64 now,
            [in] IArrayMap* pkgList);

        AddPss(
            [in] Int64 pss,
            [in] Int64 uss,
            [in] Boolean always,
            [in] IArrayMap* pkgList);

        ReportExcessiveWake(
            [in] IArrayMap* pkgList);

        ReportExcessiveCpu(
            [in] IArrayMap* pkgList);

        ReportCachedKill(
            [in] IArrayMap* pkgList,
            [in] Int64 pss);

        GetCommonProcess(
            [out] IProcessState** commonProcess);

        SetCommonProcess(
            [in] IProcessState* commonProcess);

        GetPackage(
            [out] String* package);

        GetUid(
            [out] Int32* uid);

        GetVersion(
            [out] Int32* vers);

        GetTmpTotalTime(
            [out] Int64* tmpTotalTime);

        SetTmpTotalTime(
            [in] Int64 tmpTotalTime);
    }

    [local]
    interface IProcessStateHolder {
        GetAppVersion(
            [out] Int32* appVersion);

        GetState(
            [out] IProcessState** state);

        SetState(
            [in] IProcessState* state);
    }

    /**
     * @Involve
     * interface IDurationsTable
     */
    [local]
    interface IServiceState {
        const Int32 SERVICE_RUN = 0;
        const Int32 SERVICE_STARTED = 1;
        const Int32 SERVICE_BOUND = 2;
        const Int32 SERVICE_EXEC = 3;

        ApplyNewOwner(
            [in] IInterface* newOwner);

        ClearCurrentOwner(
            [in] IInterface* owner,
            [in] Boolean silently);

        IsInUse(
            [out] Boolean* isInUse);

        IsRestarting(
            [out] Boolean* isRestarting);

        SetStarted(
            [in] Boolean started,
            [in] Int32 memFactor,
            [in] Int64 now);

        SetRestarting(
            [in] Boolean restarting,
            [in] Int32 memFactor,
            [in] Int64 now);

        SetBound(
            [in] Boolean bound,
            [in] Int32 memFactor,
            [in] Int64 now);

        SetExecuting(
            [in] Boolean executing,
            [in] Int32 memFactor,
            [in] Int64 now);

        GetPackage(
            [out] String* package);

        GetProcessName(
            [out] String* processName);

        GetRunState(
            [out] Int32* runState);

        SetRunState(
            [in] Int32 runState);

        GetStartedState(
            [out] Int32* startedState);

        SetStartedState(
            [in] Int32 startedState);

        GetBoundState(
            [out] Int32* boundState);

        SetBoundState(
            [in] Int32 boundState);

        GetExecState(
            [out] Int32* execState);

        SetExecState(
            [in] Int32 execState);
    }

    [local]
    interface IPackageState {
